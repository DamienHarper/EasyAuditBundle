{
    "name": "xiidea/easy-audit",
    "type": "symfony-bundle",
    "description": "A Symfony2 Bundle To Log Selective Events. It is easy to configure and easy to customize for your need",
    "keywords": ["Event Log", "Audit Log"],
    "license": "MIT",
    "homepage": "https://github.com/xiidea/easy-audit",
    "authors": [
        {
            "name": "Roni Saha",
            "email": "roni@xiidea.net",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/xiidea/easy-audit/issues",
        "source": "https://github.com/xiidea/easy-audit"
    },
    "require": {
        "php": ">=5.3.2"
    },
    "autoload": {
        "psr-0": {
            "Xiidea\\EasyAuditBundle": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
